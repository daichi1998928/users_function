<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<%= form_for(:session, url: login_path) do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email, class: "form_control" %>

  <%= f.label :password %>
  <%= f.password_field :password, class: "form_control" %>

  <%= f.submit "Log in", class: "btn" %>

<% end %>

<p>New User?<%= link_to "Sign_up now", signup_path %></p>
